{
  "name": "express-starter",
  "version": "1.0.0",
  "description": "A starter pack for building a Express web application with TypeScript, including pre-configured ESLint, Prettier, and Nodemon.",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "npx nodemon src/server.ts",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thezathe/express-starter.git"
  },
  "keywords": [
    "fastify",
    "typescript",
    "postgres"
  ],
  "author": "Uchkun Rakhimov <uchkunrakhimov@icloud.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/thezathe/express-starter/issues"
  },
  "homepage": "https://github.com/thezathe/express-starter#readme",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.19.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.14.2",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.57.0",
    "nodemon": "^3.1.3",
    "prettier": "^3.3.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  }
}
